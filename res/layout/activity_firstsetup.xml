<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"

    tools:context="com.example.rihaf.diabetesdietexpert.FirstSetupActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Diet Plan anda masih kosong. "
        android:id="@+id/textView36"
        android:textStyle="bold"
        android:textColor="#d66060"
        android:layout_marginTop="108dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Masukkan Data Antropometri anda untuk mengetahui jenis diet diabetes yang sesuai untuk anda."
        android:id="@+id/textView37"
        android:textAlignment="center"
        android:layout_marginTop="39dp"
        android:layout_below="@+id/textView36"
        android:layout_alignParentEnd="true" />

    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/ic_local_dining_black_24dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"></FrameLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:text="  Atur Data Antropometri Saya "
        android:layout_below="@+id/textView37"
        android:textAllCaps="false"
        android:textAlignment="center"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="51dp"
        android:id="@+id/buttonhome"
        android:textColor="#ffffff"
        android:textSize="@dimen/abc_text_size_large_material"
        android:background="@color/colorAccent" />


</RelativeLayout>
