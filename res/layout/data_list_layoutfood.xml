<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rihaf.diabetesdietexpert.DataListActivityFoodlist"
    android:background="#ffffff">

    <ListView
        android:layout_marginTop="100dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/list_view_food"
        android:background="#e1e7ff"></ListView>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="   Tambahkan Daftar Makanan   "
        android:id="@+id/button_add"
        android:background="@color/colorAccent"
        android:textColor="#ffffff"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:textAllCaps="false"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="ClickAdd" />

    <TextView
        android:layout_width="85dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_height="39dp"
        android:textSize="16dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Nama"
        android:id="@+id/textView65"
        android:layout_below="@+id/button_add"
        android:layout_alignParentStart="true"
        android:background="#5e89b7"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="95dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_height="39dp"
        android:textColor="#ffffff"
        android:background="#5e89b7"
        android:textSize="16dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Porsi"
        android:id="@+id/textView66"
        android:layout_below="@+id/button_add"
        android:layout_toEndOf="@+id/textView65" />

    <TextView
        android:layout_width="43dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_height="39dp"
        android:textColor="#ffffff"
        android:background="#5e89b7"
        android:textSize="15dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Kal/Porsi"
        android:id="@+id/textView67"
        android:layout_alignTop="@+id/textView66"
        android:layout_toEndOf="@+id/textView66" />

    <TextView
        android:layout_width="43dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_height="39dp"
        android:textColor="#ffffff"
        android:background="#5e89b7"
        android:textSize="16dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Kal/gr"
        android:id="@+id/textView68"
        android:layout_alignTop="@+id/textView67"
        android:layout_toEndOf="@+id/textView67" />

    <TextView
        android:layout_width="43dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_height="39dp"
        android:textColor="#ffffff"
        android:background="#5e89b7"
        android:textSize="16dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Pro"
        android:id="@+id/textView69"
        android:layout_below="@+id/button_add"
        android:layout_toEndOf="@+id/textView68" />

    <TextView
        android:layout_width="50dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_height="39dp"
        android:textColor="#ffffff"
        android:background="#5e89b7"
        android:textSize="16dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Lemak"
        android:id="@+id/textView70"
        android:layout_alignTop="@+id/textView69"
        android:layout_toEndOf="@+id/textView69" />

    <TextView
        android:layout_width="60dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_height="39dp"
        android:textColor="#ffffff"
        android:background="#5e89b7"
        android:textSize="16dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Karbo"
        android:id="@+id/textView71"
        android:layout_alignTop="@+id/textView70"
        android:layout_toEndOf="@+id/textView70" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/txt_mealtime2"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/textView71"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/currentcal"
        android:layout_alignTop="@+id/button_add"
        android:layout_alignParentStart="true"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/kalgoldiet"
        android:layout_alignBottom="@+id/txt_mealtime2"
        android:layout_toStartOf="@+id/textView66"
        android:textColor="#ffffff" />
</RelativeLayout>
